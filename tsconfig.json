{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "lib": [
      "ES2021",
      "dom"
    ],
    "outDir": "./dist",
    "baseUrl": "./",
    "sourceMap": true,
    "declaration": true,
    "declarationMap": false,
    "noEmit": false,
    "preserveWatchOutput": true,
    "emitDeclarationOnly": false,
    "importHelpers": false,
    "strict": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "types": ["node"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "incremental": false
  }
}
